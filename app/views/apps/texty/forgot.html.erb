<div class="jumbotron text-center">
  <div class="container">
 <h1> Enter your phone number and we will send verfication code again </h1>
 <h1> <%= @phone.number  %></h1>

  </div>
  </div>
<br><br><br><br>
  <div class="container fade" id="app-texty-form-verify-go4">

    <%= form_for @phone, url: '/apps/texty/update_phone'  do |f| %>
    <%= f.label :number, 'Enter your phone number' %>
    <%= f.text_field :number, class: 'form-control', placeholder: '############' %>




    <br>
    <%= f.submit 'Send', class: 'btn btn-primary'%>
    <% end %>
  </div>
